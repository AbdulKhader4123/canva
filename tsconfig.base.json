{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@canva/design/data-access": ["libs/design/data-access/src/index.ts"],
      "@canva/design/feature-create-design": [
        "libs/design/feature-create-design/src/index.ts"
      ],
      "@canva/design/models": ["libs/design/models/src/index.ts"],
      "@canva/design/ui": ["libs/design/ui/src/index.ts"],
      "@canva/layout/data-access": ["libs/layout/data-access/src/index.ts"],
      "@canva/layout/feature-layout": [
        "libs/layout/feature-layout/src/index.ts"
      ],
      "@canva/layout/feature-view-builder": [
        "libs/layout/feature-view-builder/src/index.ts"
      ],
      "@canva/layout/models": ["libs/layout/models/src/index.ts"],
      "@canva/layout/ui": ["libs/layout/ui/src/index.ts"],
      "@canva/shared/models": ["libs/shared/models/src/index.ts"],
      "@canva/shared/ui": ["libs/shared/ui/src/index.ts"],
      "@canva/shared/utils": ["libs/shared/utils/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
